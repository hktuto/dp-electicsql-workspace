    "postinstall": "nuxt prepare",
    "db:generate": "npx nuxt db generate",
    "db:migrate": "npx nuxt db migrate",
    "db:seed": "curl -X POST http://localhost:3000/api/dev/seed -H 'x-dev-secret: docpal-dev-secret'",
